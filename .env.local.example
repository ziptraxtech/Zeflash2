# Local Development Environment Configuration
# Copy this file to .env.local in the zeflash-new directory for local testing

# ML Backend Configuration
# Use 'auto' to automatically detect local vs production based on dev/build mode
# Or specify the exact URL for the ML backend
VITE_ML_BACKEND_URL=auto

# Enable debug logging
VITE_DEBUG=true

# Frontend port (if different from default)
VITE_PORT=5173
